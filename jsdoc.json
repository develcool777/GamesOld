{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": "src/model/",
      "includePattern": "\\.js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "template": "node_modules/docdash",
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true
  },
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "commonNav": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu":{
        "Github repo": {
            "href":"https://github.com/develcool777/Games",
            "target":"_blank",
            "class":"menu-item",
            "id":"repository"
        }
    }
  }
}